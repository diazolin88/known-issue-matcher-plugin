{
  "manifest_version": 3,
  "name": "Known Issues Matcher",
  "version": "1.0",
  "description": "Matches known error patterns on a test page.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.testmo.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.testmo.net/*"
      ],
      "js": [
        "api-client.js",
        "settings.js",
        "ui-utils.js",
        "settings-ui.js",
        "actions.js",
        "matcher.js",
        "content.js",
        "observer.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}